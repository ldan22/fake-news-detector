; Calcium strengthens bones
(subclass PureSubstance AutonomousAgent)
(exists (?C ?I)
    (and
    (instance ?C Calcium)
    (instance ?I Increasing)
    (instance ?B Bone)
    (agent ?I ?C)
    (patient ?I ?B)))

; Logic behind: If someone is eating food containing calcium then its bones strengthen
(=>
    (and
        (instance ?H Human)
        (instance ?E Eating)
        (agent ?E ?H)
        (instance ?F Food)
        (resource ?E ?F)
        (instance ?C Calcium)
        (part ?F ?C))
    (exists
        (?I ?B)
        (and
            (instance ?I Increasing)
            (causes ?E ?I)
            (instance ?B Bone)
            (part ?B ?H)
            (patient ?I ?B))))


;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;

; Proteins make muscles

(subclass Nutrient AutonomousAgent)
(exists (?P ?M)
    (and
        (instance ?P Protein)
        (instance ?M Making)
        (agent ?M ?P)
        (patient ?M Muscle)))

; A more expressive form

(=>
    (and
        (instance ?H Human)
        (instance ?E Eating)
        (agent ?E ?H)
        (instance ?F Food)
        (resource ?E ?F)
        (instance ?P Protein)
        (part ?F ?P))
    (exists
        (?M ?M2)
        (and
            (instance ?M Making)
            (causes ?E ?M)
            (instance ?M2 Muscle)
            (part ?M2 ?H)
            (patient ?M ?M2))))

